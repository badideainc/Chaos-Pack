{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "chaospack:ent",
			"materials": {
				"default": "iron_golem"
			},
			"textures": {
				"default": "textures/entity/ent"
			},
			"geometry": {
				"default": "geometry.ent"
			},
			"animations": {
				"walk": "animation.ent.walk",
				"move": "animation.ent.move",
				"walk_to_target": "animation.ent.walk_to_target",
				"move_to_target": "animation.ent.move_to_target",
				"attack": "animation.ent.attack",
				"look_at_target": "animation.common.look_at_target",
				"move_controller": "controller.animation.ent.move",
				"arm_controller": "controller.animation.ent.arm_movement"
			},

			"scripts": {
				"pre_animation": [
					"variable.modified_tcos0 = Math.clamp(((Math.cos(query.modified_distance_moved * 13.5) * Math.min(query.modified_move_speed, 0.6) / variable.gliding_speed_value) * 25.0), -12.5, 12.5);"
				],
				"animate": ["look_at_target", "move_controller", "arm_controller"]
			},
			"render_controllers": [ "controller.render.default" ],
			"spawn_egg": {
				"base_color": "#552600",
				"overlay_color": "#65dd62"
			}
		}
	}
}